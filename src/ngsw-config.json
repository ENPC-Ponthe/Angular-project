{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/images/fav-logo.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/*.ts",
          "/*.scss",
          "/manifest.webmanifest"
        ],
        "urls": [
          "https://fonts.googleapis.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|JPG|png|webp|gif|otf|ttf|woff|woff2|ani|mp4|json)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data",
      "urls": [
        "https://ponthe.enpc.org/api/members",
        "https://ponthe.enpc.org/api/cgu",
        "https://ponthe.enpc.org/api/get-latest-galleries",
        "https://ponthe.enpc.org/api/get-random-user-reactions",
        "https://ponthe.enpc.org/api/get-all-galleries",
        "https://ponthe.enpc.org/api/get-private-galleries",
        "https://ponthe.enpc.org/api/get-images/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "5d"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "/galeries/**",
    "/galeries/pics/**",
    "/galeries/movies/**",
    "!/api/**",
    "!/v1/**",
    "!/phpmyadmin/**"
  ]
}